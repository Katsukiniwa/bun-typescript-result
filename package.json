    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@tsconfig/bun": "1.0.9",
    "@tsconfig/strictest": "2.0.6",
    "@types/bun": "1.3.0"
  },
  "peerDependencies": {
    "typescript": "5.9.2"
  },
  "dependencies": {
    "neverthrow": "^8.2.0"
  }
}
